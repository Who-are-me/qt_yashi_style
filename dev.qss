QToolTip {
     border: 1px solid black;
     border-radius: 5px;
     background-color: #393538;
     padding: 1px;
     border-radius: 2px;
     opacity: 100;
}


/* QWidget */

QWidget {
    color: #EEE0B7;
    background-color: #262124;
}
QWidget:item:hover {
    background-color: #EEE0B7;
    color: #000000;
}
QWidget:item:selected {
    background-color: #808080;
}
QWidget:disabled {
    color: #404040;
    background-color: #323232;
}
QWidget:focus {
    border: 1px solid #EEE0B7;
    border-radius: 5px;
}


/* QMenuBar */

QMenuBar::item {
    background: transparent;
}
QMenuBar::item:selected {
    background: transparent;
    border: 1px solid #EEE0B7;
}
QMenuBar::item:pressed {
    background: #444;
    border: 1px solid #000;
    background-color: QLinearGradient(
        x1:0, y1:0,
        x2:0, y2:1,
        stop:1 #212121,
        stop:0.4 #343434/*,
        stop:0.2 #343434,
        stop:0.1 #EEE0B7*/
    );
    margin-bottom:-1px;
    padding-bottom:1px;
}


/* QMenu */

QMenu {
    border: 1px solid #000;
}
QMenu::item {
    padding: 2px 20px 2px 20px;
}
QMenu::item:selected {
    color: #000000;
}



QAbstractItemView {
    background-color: #606060;
}




QLineEdit {
    background-color: QLinearGradient( x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #4d4d4d, stop: 0 #646464, stop: 1 #5d5d5d);
    padding: 1px;
    border-style: solid;
    border: 1px solid #1e1e1e;
    border-radius: 5;
}


/* QPushButton */

QPushButton {
    color: #b1b1b1;
    background-color: #4E4E4E;
    border-width: 1px;
    border-color: #1e1e1e;
    border-style: solid;
    border-radius: 6;
    padding: 3px;
    font-size: 12px;
    padding-left: 5px;
    padding-right: 5px;
}
QPushButton:pressed {
    background-color: #606060;
}


/* QComboBox */

QComboBox {
    background-color: #606060;
    border-style: solid;
    border: 2px solid #1e1e1e;
    border-radius: 5;
}
QComboBox:hover,QPushButton:hover {
    border: 1px solid #EEE0B7;
}
QComboBox:on {
    padding-top: 3px;
    padding-left: 4px;
    background-color: #606060;
    selection-background-color: #606060;
}
QComboBox QAbstractItemView {
    border: 2px solid #EEE0B7;
    background-color: #606060;
    selection-background-color: #606060;
}
QComboBox::drop-down {
     subcontrol-origin: padding;
     subcontrol-position: top right;
     width: 15px;
     border-left-width: 0px;
     border-left-color: darkgray;
     border-left-style: solid; /* just a single line */
     border-top-right-radius: 4px; /* same radius as the QComboBox */
     border-bottom-right-radius: 4px;
}
QComboBox::down-arrow {
     image: url(:/images/down_arrow.png);
}


/* QGroupBox */

QGroupBox {

}
QGroupBox:focus {
    border: 2px solid #EEE0B7;
}



/* QTreeView */

QTreeView {
	background-color: #252024;
}



/* QColumnView */

QColumnView {
    background-color: #252024;
}



QTextEdit:focus {
    border: 1px solid #EEE0B7;
}

QScrollBar:horizontal {
     border: 1px solid #222222;
     background: QLinearGradient( x1: 0, y1: 0, x2: 0, y2: 1, stop: 0.0 #121212, stop: 0.2 #282828, stop: 1 #484848);
     height: 7px;
     margin: 0px 16px 0 16px;
}

QScrollBar::handle:horizontal {
      background: QLinearGradient( x1: 0, y1: 0, x2: 1, y2: 0, stop: 0 #EEE0B7, stop: 0.5 #EEE0B7, stop: 1 #EEE0B7);
      min-height: 20px;
      border-radius: 2px;
}

QScrollBar::add-line:horizontal {
      border: 1px solid #1b1b19;
      border-radius: 2px;
      background: QLinearGradient( x1: 0, y1: 0, x2: 1, y2: 0, stop: 0 #EEE0B7, stop: 1 #EEE0B7);
      width: 14px;
      subcontrol-position: right;
      subcontrol-origin: margin;
}

QScrollBar::sub-line:horizontal {
      border: 1px solid #1b1b19;
      border-radius: 2px;
      background: QLinearGradient( x1: 0, y1: 0, x2: 1, y2: 0, stop: 0 #EEE0B7, stop: 1 #EEE0B7);
      width: 14px;
     subcontrol-position: left;
     subcontrol-origin: margin;
}

QScrollBar::right-arrow:horizontal, QScrollBar::left-arrow:horizontal {
      border: 1px solid black;
      width: 1px;
      height: 1px;
      background: white;
}

QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
      background: none;
}

QScrollBar:vertical {
      background: QLinearGradient( x1: 0, y1: 0, x2: 1, y2: 0, stop: 0.0 #121212, stop: 0.2 #282828, stop: 1 #484848);
      width: 7px;
      margin: 16px 0 16px 0;
      border: 1px solid #222222;
}

QScrollBar::handle:vertical {
      background: QLinearGradient( x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #EEE0B7, stop: 0.5 #EEE0B7, stop: 1 #EEE0B7);
      min-height: 20px;
      border-radius: 2px;
}

QScrollBar::add-line:vertical {
      border: 1px solid #1b1b19;
      border-radius: 2px;
      background: QLinearGradient( x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #EEE0B7, stop: 1 #EEE0B7);
      height: 14px;
      subcontrol-position: bottom;
      subcontrol-origin: margin;
}

QScrollBar::sub-line:vertical {
      border: 1px solid #1b1b19;
      border-radius: 2px;
      background: QLinearGradient( x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #EEE0B7, stop: 1 #EEE0B7);
      height: 14px;
      subcontrol-position: top;
      subcontrol-origin: margin;
}

QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical {
    border: 1px solid black;
    width: 1px;
    height: 1px;
    background: white;
}

QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
    background: none;
}

QTextEdit {
    background-color: #2b2b2b;
}

QPlainTextEdit {
    background-color: #2b2b2b;
}




QHeaderView::section {
    background-color: #606060;
    color: white;
    padding-left: 4px;
    border: 1px solid #6c6c6c;
}




QCheckBox:disabled {
    color: #414141;
}

QDockWidget::title {
    text-align: center;
    spacing: 3px; /* spacing between items in the tool bar */
    background-color: QLinearGradient(x1:0, y1:0, x2:0, y2:1, stop:0 #323232, stop: 0.5 #042424, stop:1 #323232);
}

QDockWidget::close-button, QDockWidget::float-button {
    text-align: center;
    spacing: 1px; /* spacing between items in the tool bar */
    background-color: QLinearGradient(x1:0, y1:0, x2:0, y2:1, stop:0 #323232, stop: 0.5 #042424, stop:1 #323232);
}

QDockWidget::close-button:hover, QDockWidget::float-button:hover {
    background: #042424;
}

QDockWidget::close-button:pressed, QDockWidget::float-button:pressed {
    padding: 1px -1px -1px 1px;
}

QMainWindow::separator {
    background-color: QLinearGradient(x1:0, y1:0, x2:0, y2:1, stop:0 #161616, stop: 0.5 #151515, stop: 0.6 #212121, stop:1 #343434);
    color: white;
    padding-left: 4px;
    border: 1px solid #4c4c4c;
    spacing: 3px; /* spacing between items in the tool bar */
}

QMainWindow::separator:hover {
    background-color: QLinearGradient(x1:0, y1:0, x2:0, y2:1, stop:0 #EEE0B7, stop:0.5 #b56c17 stop:1 #EEE0B7);
    color: white;
    padding-left: 4px;
    border: 1px solid #6c6c6c;
    spacing: 3px; /* spacing between items in the tool bar */
}

QToolBar::handle {
     spacing: 3px; /* spacing between items in the tool bar */
     background: url(:/resource/handle.png);
}

QMenu::separator {
    height: 2px;
    background-color: QLinearGradient(x1:0, y1:0, x2:0, y2:1, stop:0 #161616, stop: 0.5 #151515, stop: 0.6 #212121, stop:1 #343434);
    color: white;
    padding-left: 4px;
    margin-left: 10px;
    margin-right: 5px;
}

QProgressBar {
    border: 2px solid grey;
    border-radius: 5px;
    text-align: center;
}

QProgressBar::chunk {
    background-color: #EEE0B7;
    width: 2.15px;
    margin: 0.5px;
}












