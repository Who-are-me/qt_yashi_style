/* QTabWidget QTabBar  */

QTabWidget::tab-bar {
    alignment: center;
}

QTabBar::tab {
    border-bottom: 1px solid #606060;
    border-top: 1px solid #808080;
    padding: 0.29em;
    color: #EEE0B7;
    background-color: #606060;
}
QTabBar::tab:!last {
    border-right: 1px solid;
    border-right-color: #393538;
    /*border-bottom-color: #c0c0c0;*/
}
QTabBar::tab:first {
    border-top-left-radius: 7px;
    border-bottom-left-radius: 7px;
}
QTabBar::tab:last {
    border-top-right-radius: 7px;
    border-bottom-right-radius: 7px;
}
QTabBar::tab:selected, QTabBar::tab:last:selected, QTabBar::tab:hover {
    color: #FFFFFF;
    background-color: #808080;
}

















QRadioButton::indicator:checked, QRadioButton::indicator:unchecked{
    color: #b1b1b1;
    background-color: #323232;
    border: 1px solid #b1b1b1;
    border-radius: 5px;
}

QRadioButton::indicator:checked {
    background-color: qradialgradient(
        cx: 0.5, cy: 0.5,
        fx: 0.5, fy: 0.5,
        radius: 1.0,
        stop: 0.25 #EEE0B7,
        stop: 0.3 #323232
    );
}

QCheckBox::indicator{
    color: #b1b1b1;
    background-color: #323232;
    border: 1px solid #b1b1b1;
    border-radius: 2px;
    width: 9px;
    height: 9px;
}

QRadioButton::indicator {
    border-radius: 5px;
}

QRadioButton::indicator:hover, QCheckBox::indicator:hover {
    border: 1px solid #fff;
}

QCheckBox::indicator:checked {
    image:url(:/images/checkbox.png);
    background-color: #EEE0B7;
}

QCheckBox::indicator:disabled, QRadioButton::indicator:disabled {
    border: 1px solid #ab3535;
} 

QRadioButton:checked:enabled {
    border: 0px solid #fff;
}

QRadioButton:!checked {
    border: 0px solid #fff;
}

QCheckBox {
    border: 0px solid #fff;
}

QCheckBox:!checked {
    border: 0px solid #fff;
}

QCheckBox:checked:enabled {
    border: 0px solid #fff;
}

QCheckBox:selected {
    border: 0px solid #fff;
}




QToolBar {
    background-color: #393538;
    border-top: 0px solid #fff;
    border-bottom: 1px solid #363636;
}

QToolBar QAction {
    border: 1px solid #fff;
}

QToolBar QLineEdit {
    border: 1px solid #363636;
}

QToolButton {
    border: 1px solid #363636;
    color: #b1b1b1;
    background-color: QLinearGradient( x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #565656, stop: 0.1 #525252, stop: 0.5 #4e4e4e, stop: 0.9 #4a4a4a, stop: 1 #464646);
    border-width: 1px;
    border-color: #1e1e1e;
    border-style: solid;
    border-radius: 6;
    padding: 3px;
    font-size: 12px;
    padding-left: 5px;
    padding-right: 5px;
}

QToolButton:hover {
    border: 1px solid #EEE0B7;
}

QToolButton:pressed {
    background-color: QLinearGradient( x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #2d2d2d, stop: 0.1 #2b2b2b, stop: 0.5 #292929, stop: 0.9 #282828, stop: 1 #252525);
}

QStatusBar {
    background-color: #393538;
}


QListWidget {
    background-color: #393538;
}

QListView {
    background-color: #393538;
}

QTableView {
    background-color: #393538;
}













QSpinBox {
        border: 1px solid #616161;
        border-radius: 5px;
}


QSpinBox::up-arrow, QDoubleSpinBox::up-arrow, QTimeEdit::up-arrow, QDateEdit::up-arrow, QDateTimeEdit::up-arrow {
        image: url(:/resource/go-up.png);
        padding: 1px;
        margin: 0px;
}

QSpinBox::up-button {
        padding: 1px;
        margin: 0px;
        background-color: #393538;
}

QSpinBox::down-arrow, QDoubleSpinBox::down-arrow, QTimeEdit::down-arrow, QDateEdit::down-arrow, QDateTimeEdit::down-arrow {
        image: url(:/resource/go-down.png);
        padding: 1px;
        margin: 0px;
}

QSpinBox::down-button {
        padding: 1px;
        margin: 0px;
        background-color: #393538;
}






